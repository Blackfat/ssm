<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <typeAliases>
    <!-- 给实体类起一个别名 -->
    <typeAlias type="com.framework.persistent.jpa.domain.User" alias="User"/>
    </typeAliases>
    
   
	
    <!-- 加载实体的sql映射语句 -->
	<mappers>
	<mapper resource="com/framework/persistent/mybatis/mapper/UserMapper.xml"/>
	</mappers>
	
</configuration>		
			